{
  "name": "jeff-at-aha.development-warnings",
  "description": "Development warnings",
  "version": "0.1.0",
  "author": "Jeff Tucker <support@aha.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/trydionel/development-warnings"
  },
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/react": "^16.8.0"
  },
  "ahaExtension": {
    "contributes": {
      "views": {
        "warnings": {
          "title": "Warnings",
          "entryPoint": "src/views/warnings.js",
          "host": "attribute",
          "recordTypes": [
            "Feature",
            "Requirement",
            "Epic",
            "Release"
          ]
        }
      },
      "settings": {
        "warningDates": {
          "title": "Warn on missing dates",
          "type": "boolean",
          "default": false,
          "scope": [
            "account"
          ]
        },
        "warningEstimates": {
          "title": "Warn on missing estimates",
          "type": "boolean",
          "default": true,
          "scope": [
            "account"
          ]
        },
        "warningLate": {
          "title": "Warn on late delivery",
          "type": "boolean",
          "default": true,
          "scope": [
            "account"
          ]
        },
        "warningAssignment": {
          "title": "Warn on unassigned work",
          "type": "boolean",
          "default": true,
          "scope": [
            "account"
          ]
        }
      }
    }
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
